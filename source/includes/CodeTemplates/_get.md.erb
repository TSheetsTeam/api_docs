<% if defined? base_url -%>
  <% @base_url = base_url -%>
<% else -%>
  <% @base_url = "https://rest.tsheets.com/api/v1/" -%>
<% end -%>

<% @full_url = "#{@base_url}#{endpoint}" -%>

<% if defined? parameters -%>
  <% @params = [] -%>
  <% parameters.each do |kvp| -%>
    <% @params.push(kvp.join('=')) -%>
  <% end -%>
  <% @full_url += "?#{@params.join('&')}" -%>  
 <% end -%>

<% if defined? headers -%>
  <% @headers = headers -%>
<% else -%>
  <% @headers = [ [ "Authorization", "Bearer <TOKEN>" ] ] -%>
<% end -%>

 > **Example**: <%= description %>

